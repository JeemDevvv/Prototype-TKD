{
  "name": "prototype-tkd",
  "version": "1.0.0",
  "description": "Taekwondo Web System",
  "main": "backend/app.js",
  "scripts": {
    "start": "node backend/app.js",
    "dev": "nodemon backend/app.js",
    "kill-port": "powershell -ExecutionPolicy Bypass -File ./kill-port.ps1",
    "dev-safe": "powershell -ExecutionPolicy Bypass -File ./kill-port.ps1 && timeout /t 1 /nobreak >nul && npm run dev",
    "start-safe": "powershell -ExecutionPolicy Bypass -File ./kill-port.ps1 && timeout /t 1 /nobreak >nul && npm start",
    "build": "node build.js",
    "build-start": "npm run build && npm start",
    "build-dev": "npm run build && npm run dev"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "exceljs": "^4.3.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.17.3",
    "mongoose": "^7.6.1",
    "multer": "^2.0.2",
    "pdfkit": "^0.13.0",
    "socket.io": "^4.8.1",
    "validator": "^13.15.20"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
